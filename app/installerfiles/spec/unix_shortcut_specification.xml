<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <programGroup defaultName="EasyTournament"
  location="applications"/>

   <shortcut
     name="EasyTournament"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="/usr/lib/java/bin/java"
     commandLine="-Xms1000m -Xmx2000m -jar
     &quot;$INSTALL_PATH/EasyTournament.jar&quot;"
     initialState="noShow"
     iconFile="$INSTALL_PATH/res/icons/app_icon.png"
     iconIndex="0"
     workingDirectory=""
     type="Application"
     encoding="UTF-8"
     description="Uninstaller">
     <createForPack name="Core" />
   </shortcut>

   <shortcut
     name="Uninstaller"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="/usr/lib/java/bin/java"
     commandLine="-jar
     &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
     initialState="noShow"
     iconFile="trashcan_full"
     iconIndex="0"
     workingDirectory=""
     type="Application"
     encoding="UTF-8"
     description="Uninstaller">
     <createForPack name="Core" />
   </shortcut>

</shortcuts>